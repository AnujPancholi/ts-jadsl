{
  "name": "ts-jadsl",
  "version": "0.0.1",
  "description": "Just another data structures library",
  "main": "src/index.ts",
  "files": ["src","dist"],
  "scripts": {
    "coverage": "NODE_ENV=test jest --coverage=true",
    "test": "NODE_ENV=test jest",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "lint": "eslint . --ext .js,.ts,.json",
    "precommit": "lint-staged",
    "build": "tsc -p ."
  },
  "keywords": [
    "data",
    "structures",
    "jadsl",
    "typescript",
    "ts",
    "algorithm"
  ],
  "author": "Anuj Pancholi",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AnujPancholi/ts-jadsl"
  },
  "bugs": {
    "url" : "https://github.com/AnujPancholi/ts-jadsl/issues"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": -10
      }
    }
  },
  "lint-staged": {
    "*.{js,ts,json}": "eslint --cache --fix",
    "*.{js,css,ts,md}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  }
}
